          </object>
        </child>
        <child>
          <!-- <object id="mainstack" class="GtkStack"> -->
            <!-- <child type="page"> -->
          <object id="page1" class="GtkBox">
          <property name="name">newlog</property>
          <child> 
              <object id="log_name_entry" class="GtkEntry">
              <property name="name">Log Name</property>
              </object>
           </child>
          </object>
          <!-- </child> 
          </object> -->
      </child> 
      </object>
  </child>
  </object>
